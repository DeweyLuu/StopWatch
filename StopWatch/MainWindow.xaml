<Window x:Class="StopWatch.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StopWatch"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Start" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="button_Click" Margin="97,224,0,0"/>
        <Button x:Name="button1" Content="Stop" HorizontalAlignment="Left" Margin="221,224,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"/>
        <Button x:Name="button2" Content="Reset" HorizontalAlignment="Left" Margin="341,224,0,0" VerticalAlignment="Top" Width="75" Click="button2_Click"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="154,67,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="72" Width="211"/>
        <Button x:Name="button3" Content="Button" HorizontalAlignment="Left" Margin="154,272,0,0" VerticalAlignment="Top" Width="75" Click="button3_Click_1"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="154,173,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>


    </Grid>
</Window>
